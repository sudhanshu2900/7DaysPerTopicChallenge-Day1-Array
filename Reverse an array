PROBLEM: arr[]= 1,2,3,4

OUTPUT: 4,3,2,1

SOLUTION:

#include <iostream>
using namespace std;

int main() {
	int t;
	int n, arr[100];
	cin>>t;
	
	while(t--){
	    
	    cin>>n;
	    
	    for(int i=0;i<n;i++){
	        cin>>arr[i];
	    }
	    
	    for(int i=n-1;i>=0;i--){
	        cout<<arr[i]<<" ";
	    }
	    cout<<endl;
	}
	return 0;
}


